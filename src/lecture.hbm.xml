<?xml version="1.0"?>

		<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
		
<hibernate-mapping>
	<class name="com.sware.hyb.Lecture" table="lecture">
		<id name="lecture_id" column="lect_id"></id>
		<property name="subject" column="subject"></property>
		<property name="lectureTime" column="lectTime"></property>
	</class>
</hibernate-mapping>